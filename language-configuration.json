{
  "comments": { "blockComment": ["/*", "*/"] },
  "brackets": [["(", ")"], ["[", "]"]],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "\"", "close": "\"" }
  ],
  "surroundingPairs": [["(", ")"], ["[", "]"], ["\"", "\""]],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(?:Script|Function|Action)\\s+\\d+:\\s*$",
    "decreaseIndentPattern": "^\\s*(?:End|Return|Jump\\b|UseScript#\\d+)\\b"
  }
}
