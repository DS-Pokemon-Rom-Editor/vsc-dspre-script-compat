{
    "scopeName": "source.customscript",
    "patterns": [
        {
            "name": "entity.name.control.customscript",
            "match": "\\b(Script|Function|Action|End|Return)\\b"
        },
        {
            "name": "constant.numeric.customscript", 
            "match": "\\b\\d+\\b"
        },
        {
            "name": "constant.numeric.prefixed.customscript", 
            "match": "#\\d+"
        },
        {
            "name": "keyword.operator.customscript", 
            "match": "\\b(LESS|EQUAL|GREATER|LESS/EQUAL|GREATER/EQUAL|DIFFERENT)\\b"
        },
        {
            "name": "support.function.cmd.customscript", 
            "match": "\\bCMD_\\d+\\b"
        },
        {
            "name": "text.other.customscript", 
            "match": "\\b\\w+\\b"
        },
        {
            "name": "comment.line.double-slash.customscript",
            "begin": "##",
            "end": "$",
            "captures": {
                "0": {
                    "name": "comment.line.double-slash.customscript"
                }
            }
        }
    ]
}
